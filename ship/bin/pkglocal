#!/bin/ksh

#v=$(date -f %Y-%m-%d)
#v=$(< version)
v=1100.2025.08.20
[[ $v == '' ]] && exit

print $v

rm -f swift.v$v.*tgz swift.v$v.*.full.tar

archs=$(egrep ssh: ../lib/package/admin/admin.db | egrep -v '^#' | sed 's/[ 	].*$//')

for a in $archs; do
    ./bin/mksw $a swift.v$v INIT ast swift
done
